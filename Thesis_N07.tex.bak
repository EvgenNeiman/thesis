\subsubsection{Формальні ряди та матриці Тепліца}

Наступна допоміжна умова була доведена в роботі \cite[Lemma
2.12]{D09} у більш загальному сенсі.
\begin{lemma}(\cite{D09})\label{7.lem:6.1} Нехай $m_0\in N$, тоді
\begin{enumerate}
\item[(i)]
$f(\lambda)=O(1)$ ($\lambda\widehat{\rightarrow}\infty$) для всіх
$f\in\cH(m_0)$;
\item[(ii)]
Якщо до того ж, $m_0\in N^0$ тоді $f(\lambda)=O(\frac{1}{\lambda})$
($\lambda\widehat{\rightarrow}\infty$) для всіх $f\in\cH(m_0)$.
\end{enumerate}
\end{lemma}

Будемо позначати $\cP_n$ множину поліномів формального ступеню $n$.

Доведемо алгебраїчне твердження, яке стосується формальних
степеневих рядів $p(\l)$ ступеню $n$
\begin{equation}\label{7.7.0}
p(\l)=p_0\l^n+p_1\l^{n-1}+...
\end{equation}
і відповідних матриць Тепліца $T_k(p)\in\dC^{k\times k}$ їх $k$
старших коефіцієнтів
\begin{equation}\label{7.7.1}
T_k(p):=\begin{bmatrix}
p_0 & p_1 & p_2 & \cdots & p_{k-1}\\
0 & p_0   & p_1 & \cdots & p_{k-2}\\
0 & 0   & p_0   & \cdots & p_{k-3}\\
\vdots &  &  & \ddots & \vdots \\
0 & 0   & 0 & \cdots & p_0 \\
\end{bmatrix}
\end{equation}

\begin{proposition}\label{7.pr:7.1}
Нехай $p$ і $q$ -- це формальні ряди ступенів $k$ і $n$, відповідно,
$$
p(\l)=p_0\l^k+p_1\l^{k-1}+...,\ \ \
q(\l)=q_0\l^n+q_1\l^{n-1}+...\quad (p_0,q_0\not=0).
$$
Нехай $T_j(p)$ і $T_j(q)$ є матрицями $j$ старших коефіцієнтів рядів
$p$ і $q$. Тоді
\begin{equation}\label{7.7.1a}
T_j(pq)=T_j(p)T_j(q).
\end{equation}
\end{proposition}

\begin{proof}
Добуток $T_j(p) T_j(q)$ дорівнює

{\small
\begin{equation}\label{7.7.2}
T_j(p) T_j(q)=
\begin{bmatrix}
p_0q_0 & p_0q_1+p_1q_0 & p_0q_2+p_1q_1+p_2q_0 & \cdots & p_0q_{j-1}+p_1q_{j-2}+...+p_{j-1}q_0\\
0      & p_0q_0        & p_0q_1+p_1q_0        & \cdots & p_0q_{j-2}+p_1q_{j-3}+...+p_{j-2}q_0\\
0      &   0           & p_0q_0               & \cdots & p_0q_{j-3}+p_1q_{j-4}+...+p_{j-3}q_0\\
\vdots &               &                      & \ddots & \vdots\\
0      &   0           &   0                  & \cdots & p_0q_0\\
\end{bmatrix}.
\end{equation}
}

Добуток рядів $p(\l)q(\l)$ має форму
\begin{equation}\label{7.7.3}
\begin{aligned}
p(\l)q(\l)&=p_0q_0\l^{k+n} + (p_0q_1+p_1q_0)\l^{k+n-1} +
(p_0q_2+p_1q_1+p_2q_0)\l^{k+n-2} +...\\
&+ (p_0q_{j-1}+p_1q_{j-2}+...+p_{j-1}q_0)\l^{k+n-j+1} +
o(\l^{k+n-j+1}).
\end{aligned}
\end{equation}
Отже формула~\eqref{7.7.1a} доведена.
\end{proof}

\begin{corollary}\label{7.cor:7.1a}
Так як добутки $p(\l)q(\l)$ і $q(\l)p(\l)$ формальних рядів
співпадають, то матриці $T_j(p)$ і $T_j(q)$ комутують
$$
T_j(p) T_j(q)=T_j(q) T_j(p).
$$
\end{corollary}

\begin{corollary}\label{7.cor:7.2}
Нехай $q(\l)=q_0\l^n+q_1\l^{n-1}+...$ формальний ряд з $q_0\not=0$.
Тоді $j$ старших коефіцієнтів ряду
$$
\frac1{q(\l)}=\frac{q_0'}{\l^n}+\frac{q_1'}{\l^{n+1}}+\frac{q_2'}{\l^{n+2}}+...
$$
будуються по матриці $T_j(1/q)$, яка пов'язана з $T_j(q)$
співвідношенням $T_j\bs(){1/q}=\bs(){T_j(q)}^{-1}$.
\end{corollary}

\begin{proposition}\label{7.pr:7.1a}
Нехай $c(\l)=\l^j+c_1\l^{j-1}+...+c_j$ є поліномом ступеню
 $j$. Нехай функція $m_0\in N^0$ допускає інтегральне зображення~\eqref{7.2.01a}
і допускає розклад у ряд~\eqref{7.2.03}. Тоді функція
\begin{equation*}%\label{7.7.3a}
d(\l)=\int_{-\oo}^\oo\frac{c(t)-c(\l)}{t-\l}d\s(t)
\end{equation*}
є поліномом від $\l$ ступеню $j-1$ і
\begin{equation}\label{7.7.3c}
T_j(d)=T_j(c)T_j(s^0),
\end{equation}
де $T_j(c)$ і $T_j(s^0)$ матриці $j$ старших коефіцієнтів поліномів
$c(\l)$ і
$$
s^0(\l)=s_0^0\l^{j-1}+s^0_1\l^{j-2}+...+s^0_j.
$$
\end{proposition}

\begin{proof}
Покладемо $c_0=1$. Тоді функція $d(\l)$ має форму
\begin{equation}\label{7.7.3b}
\begin{aligned}
&d(\l)=\int_{-\oo}^\oo
(\frac{t^j-\l^j}{t-\l}c_0+\frac{t^{j-1}-\l^{j-1}}{t-\l}c_1
+...+\frac{t-\l}{t-\l}c_{j-1})d\s(t)\\
&=\int_{-\oo}^\oo \bs(){c_{0}(t^{j-1}+t^{j-2}\l+...+\l^{j-1})+c_1(
t^{j-2}+...+\l^{j-2})+...+c_{j-1}}d\s(t).
\end{aligned}
\end{equation}
Як випливає з рівностей \eqref{7.2.04} та \eqref{7.7.3b}
\begin{equation*}
\begin{aligned}
d(\l)&= c_{0}(s^0_{j-1}+...+s_0^0\l^{j-1})+c_1(
s^0_{j-2}+...+s_0^0\l^{j-2})+...+c_{j-1}s_0^0\\
&=c_0s_0^0\l^{j-1}+(c_0s_1^0+c_1s_0^0)\l^{j-2}+...+(c_0s_{j-1}^0+c_1s_{j-2}^0+...+
c_{j-1}s_0^0).
\end{aligned}
\end{equation*}
Отже з \eqref{7.7.2} випливає \eqref{7.7.3c}.
\end{proof}

%------------ 7 new subsubsection -----------------------------------------

\subsubsection{Одна допоміжна лема}

\begin{lemma}\label{7.lem:7.3a}
Функція $m\in N_\k$ задовольняє умову
\begin{equation}\label{7.7.a1}
m(\l)=-\frac{s_0}{\l}-\frac{s_1}{\l^2}-\frac{s_2}{\l^3}-...
-\frac{s_{j-1}}{\l^{j}}+O(\frac1{\l^{j+1}}),\ \ \ \ \ \l\wh\to\oo \
\ (\mbox{для всіх } j\in\dZ)
\end{equation}
 тоді і тільки тоді, коли
\begin{equation}\label{7.7.a2}
\l^j m(\l)+s_0\l^{j-1}+s_1\l^{j-2}+..+s_{j-1}\in\cH(m)\ \ \
(\mbox{для всіх } j\in\dZ).
\end{equation}
\end{lemma}

\begin{proof}
\emph{Достатність.} Припустимо, що функція $m\in N_\k$ задовольняє
\eqref{7.7.a2}. Покажемо, що умова~\eqref{7.7.a1} виконана. Нехай
поліноми $p$, $q$ і функція $m_0\in N$ визначені
факторизацією~\eqref{7.3.1} для функції $m\in N_\k$.

\textbf{\emph{Випадок 1.}} Нехай $\k=n=\deg q>\deg p=k$. Тоді
$r(\l)=O(1/\l)$ при $\l\wh{\to}\oo$. Так як $m_0\in N$, то
\begin{equation}\label{7.6.a1}
m_0(\l)=O(\l)\ \ \ \ (\l\wh\to\oo).
\end{equation}
З Леми~\ref{7.lem:6.1} випливає, що
\begin{equation}\label{7.6.a2}
f_0(\l)=O(1)\ \ \ \ (\l\wh\to\oo)
\end{equation}
для усіх $f_0\in\cH(m_0)$. З формули~\eqref{7.3.3} отримуємо, що для
усіх $f\in\cH(m)$
\begin{equation}\label{7.6.a3}
f(\l)=O(1/\l)\ \ \ \ (\l\wh\to\oo).
\end{equation}
Таким чином, співвідношення \eqref{7.7.a2} означає, що
$$
m(\l)+\frac{s_0}{\l}+\frac{s_1}{\l^2}+...+\frac{s_{j-1}}{\l^j}=O(\frac1{\l^{j+1}})
\ \ \ (\l\wh\to\oo).
$$

\textbf{\emph{Випадок 2.}} Нехай $\k=\deg q=\deg p$.  Тоді
$r(\l)=O(1)$ при $\l\wh{\to}\oo$. Швидкість зростання на
нескінченності функції $m_0(\l)$ показано в \eqref{7.6.a1} та
\eqref{7.6.a2}. Тоді з Теореми~\ref{7.th:3.5} випливає, що для усіх
$f\in\cH(m)$
\begin{equation}\label{7.6.a4}
f(\l)=O(1)\ \ \ \ (\l\wh\to\oo).
\end{equation}

З формули \eqref{7.7.a2} слідує, що
$$
m(\l)=O(1/\l)\ \ \ \ (\l\wh\to\oo).
$$
Отже, з припущення $\deg q=\deg p$ та факторизації~\eqref{7.3.1}
випливає, що
\begin{equation}\label{7.6.a5}
m_0(\l)=O(1/\l)\ \ \ \ (\l\wh\to\oo).
\end{equation}
З Леми~\ref{7.lem:6.1} отримаємо, що
\begin{equation}\label{7.6.a6}
f_0(\l)=O(1/\l)\ \ \ \ (\l\wh\to\oo)
\end{equation}
для усіх $f_0\in\cH(m_0)$. Отже з \eqref{7.3.3}, \eqref{7.6.a5} і
\eqref{7.6.a6} ми маємо  \eqref{7.6.a3}. Закінчення доведення
аналогічне \emph{Випадку 1} цієї Леми.

\textbf{\emph{Випадок 3.}} Нехай $n=\deg q<\deg p=k=\k$. Тоді
$r(\l)=O(\l^{k-n})$ при $\l\wh{\to}\oo$. Як слідує з
Теореми~\ref{7.th:3.5} і співвідношень \eqref{7.6.a1},
\eqref{7.6.a2}, для усіх $f\in\cH(m)$ маємо
\begin{equation}\label{7.6.a8}
f(\l)=O(\l^{2k-2n})\ \ \ \ (\l\wh\to\oo).
\end{equation}

Умова \eqref{7.7.a2} при $j=2$ дає
$$
m(\l)=O(\l^{2k-2n-2})\ \ \ \ (\l\wh\to\oo).
$$
Таким чином з факторизації~\eqref{7.3.1} випливає, що
\begin{equation}\label{7.6.a9}
m_0(\l)=O(\l^{-2})\ \ \ \ (\l\wh\to\oo).
\end{equation}
Співвідношення~\eqref{7.6.a9} є протиріччям умові $m_0\in N$. Отже
нерівність $\deg q<\deg p$ є неможливою, для функцій $m$, які
задовольняють умови~\eqref{7.7.a2}.

\emph{Необхідність.} Нехай функція $m\in N_\k$ задовольняє
умові~\eqref{7.7.a1}. Визначимо поліном $s(\l)$ формального ступеню
$j-1$ формулою
\begin{equation}\label{7.7.4a}
s(\l):=s_0\l^{j-1}+s_1\l^{j-2}+...+s_{j-1}.
\end{equation}
Нехай функція $m_0\in N$ та монічні поліноми $p$ і $q$ визначені
факторизацією~\eqref{7.3.1}. Нехай ступені поліномів $p$ і $q$
дорівнюють $k$ та $n$, відповідно,
\begin{equation}\label{7.7.7}
\begin{aligned}
&p(\l)=\l^k+p_1\l^{k-1}+...+p_{k-1}\l+p_k,\\
&q(\l)=\l^n+q_1\l^{n-1}+...+q_{n-1}\l+q_n,
\end{aligned}
\end{equation}
і нехай функція $m_0\in N$ визначена факторизацією~\eqref{7.3.1}.

\textbf{\emph{Випадок 1: $\k=k=\deg p=\deg q=n$.}}

З умови~\eqref{7.7.a1} випливає, що $m_0=O(1/\l)$, тому, функція
$m_0$ належить до класу $N^0$ та має інтегральне
зображення~\eqref{7.2.01a}.

Нехай $c$ є деякій монічний поліном ступеню $j$ ($c_i\in\dC$ при
$i=1,2,...,j$)
\begin{equation}\label{7.7.12}
c(\l):=\l^j+c_1\l^{j-1}+c_2\l^{j-2}+...+c_{j}.
\end{equation}
Розглянемо такий розклад функції $\l^j m(\l)+s(\l)$
\begin{equation}\label{7.7.8}
\l^j
m(\l)+s(\l)=\frac{p(\l)}{q(\l)}f(\l)+\frac{p(\l)}{q(\l)q^\#(\l)}m_0(\l)\f_2(\l)
+\frac1{q(\l)}\f_1(\l),
\end{equation}
де
\begin{equation}\label{7.7.9}
f(\l):=\int_{-\oo}^\oo\frac{c(t)}{t-\l}d\s(t),
\end{equation}
\begin{equation}\label{7.7.11}
\f_1(\l):=q(\l)s(\l)+p(\l)m_0(\l)c(\l)-p(\l)\int_{-\oo}^\oo\frac{c(t)}{t-\l}d\s(t),
\end{equation}
\begin{equation}\label{7.7.10}
\f_2(\l):=p^\#(\l)\l^j-q^\#(\l)c(\l).
\end{equation}

Ми покажемо, що поліном $c$ можна вибрати таким чином, що
\begin{equation}\label{7.7.13}
f(\cdot)\in\cH(m_0);\ \ \ \f_1(\cdot),\f_2(\cdot)\in\cP_{\k-1}.
\end{equation}
Тоді за Теоремою~\ref{7.th:3.5} це буде означати, що \eqref{7.7.a2}
виконано.

Включення $f(\cdot)\in\cH(m_0)$ випливає з
Зауваження~\ref{7.rem:2.04b} і Теореми~\ref{7.th:2.3a}.

Нехай $P_i=T_i(p)$, $Q_i=T_i(q)$, $S_i=T_i(s)$, $C_i=T_i(c)$,
$S_i^0=T_i(s^0)$ матриці $i$ старших коефіцієнтів поліномів $p(\l)$,
$q(\l)$, $s(\l)$, $c(\l)$, $s^0(\l)$, відповідно, і нехай $\ol
P_{i}$, $\ol Q_{i}$ матриці з комплексно спряженими елементами до
елементів матриць $P_i$ та $Q_i$. Відмітимо, що $\ol
P_{i}=T_i(p^\#)$, $\ol Q_{i}=T_i(q^\#)$.

Функція $\f_2$ є поліномом формального ступеню $\k+j$. Визначимо
поліном $c(\l)$ так, щоб $\f_2(\cdot)\in\cP_{\k-1}$. Ця умова є
еквівалентною умові
$$
\ol P_{i}-\ol Q_{i} C_{i}=0,\ \ \ \ (i=1,2,...,j+1).
$$
Таким чином
\begin{equation}\label{7.7.16}
C_{i}=(\ol Q_{i})^{-1} \ol P_{i} = \ol P_{i}(\ol Q_{i})^{-1}\ \ \ \
(i=1,2,...,j+1).
\end{equation}
Відмітимо, що матриця $\ol Q_{j+1}$ є оборотною, а тому коефіцієнти
$c_1,c_2,...,c_{j}$ знаходяться однозначно.

Далі доведемо включення $\f_1(\cdot)\in\cP_{\k-1}$. З
факторизації~\eqref{7.3.1} випливає, що
\begin{equation}\label{7.7.19}
-S_j=P_j\ol P_j Q_j^{-1}(\ol Q_j)^{-1}(-S_j^0),
\end{equation}
при чому всі матриці в ~\eqref{7.7.19} комутують (дивись
Наслідок~\ref{7.cor:7.1a}). З інтегрального вигляду~\eqref{7.2.01a}
випливає
$$
\f_1(\l)=q(\l)s(\l)-p(\l)\int_{-\oo}^\oo\frac{c(t)-c(\l)}{t-\l}d\s(t).
$$
Пропозиція~\ref{7.pr:7.1a} означає, що $\f_1(\l)$ -- це поліном
формального ступеню $\k+j-1$ і
\begin{equation}\label{7.7.18}
T_j(\f_1)=Q_jS_j-P_jC_jS_j^0.
\end{equation}
Використовуючи формулу~\eqref{7.7.16} при $i=j$ і
співвідношення~\eqref{7.7.19} отримаємо
\begin{equation*}
\begin{aligned}
T_j(\f_1)=&Q_jP_j\ol P_j Q_j^{-1}(\ol Q_j)^{-1}S_j^0-P_j (\ol
Q_j)^{-1}\ol P_j S_j^0=0.
\end{aligned}
\end{equation*}
Таким чином, ми отримали \eqref{7.7.13}.

\textbf{\emph{Випадок 2: $k=\deg p<\deg q=n=\k$.}} Позначимо
$d:=n-k(>0)$. З розкладу~\eqref{7.2.01} для функції $m_0\in N$
випливає, що
\begin{equation}\label{7.7.21}
m_0(\l)=a\l+b+m_{00}(\l).
\end{equation}
З факторизації~\eqref{7.3.1} для $m\in N_\k$ і умови~\eqref{7.7.a1}
отримаємо, що $m_{00}(\l)=O(1/\l)$, так що $m_{00}\in N^0$. Отже
функція $m_{00}$ має інтегральне зображення, що є аналогічним
\eqref{7.2.01a}.

Нехай коефіцієнт $a$ в \eqref{7.7.21} не дорівнює $0$, тоді
$m_0(\l)=O(\l)$ при $\l\wh\to\oo$ (випадки коли $m_0(\l)=O(1)$ і
$m_0(\l)=m_{00}(\l)=O(1/\l)$ при $\l\wh\to\oo$ розглядаються
аналогічно). З факторизації~\eqref{7.3.1} випливає, що
$m(\l)=O(\frac1{\l^{2d-1}})$ при $\l\wh\to\oo$ і
\begin{equation}\label{7.7.22}
s_0=s_1=...=s_{2d-3}=0,\ \ \ s_{2d-2}\not=0
\end{equation}
($s_0=...=s_{2d-2}=0$, $s_{2d-1}\not=0$ при $a=0$;
$s_0=...=s_{2d-1}=0$, $s_{2d}\not=0$ при $a=b=0$). Більш того,
поліном $s(\l)$, що є визначеним формулою \eqref{7.7.4a}, має
ступінь $j-2d+1$
$$
s(\l)=s_{2d-2}\l^{j-2d+1}+s_{2d-1}\l^{j-2d}+...+s_{j-1}.
$$

\textit{Нехай $j\geq 2d-1$.} Розглянемо розклад~\eqref{7.7.8}
функції $\l^j m(\l)+s(\l)$. Відміна від розглянутого випадку $d=0$
полягає у тому, що функція $m_0\in N$ має зображення~\eqref{7.7.21};
міра $d\s$, знаходиться з інтегрального розкладу~\eqref{7.2.01a} для
функції $m_{00}$; поліном $c$ має ступінь $j-d$
\begin{equation}\label{7.7.23}
c(\l):=\l^{j-d}+c_1\l^{j-d-1}+c_2\l^{j-d-2}+...+c_{j-d}.
\end{equation}

Покажемо, що поліном $c$ можна вибрати так, що умова~\eqref{7.7.13}
буде виконана.

З Зауваження~\ref{7.rem:2.04b} і Теореми~\ref{7.th:2.3a} випливає
включення $f(\cdot)\in\cH(m_0)$. Умова $\f_2(\cdot)\in\cP_{\k-1}$ є
еквівалентною до умови ~\eqref{7.7.16} при $i=j-d+1$.

Далі покажемо включення $\f_1(\cdot)\in\cP_{\k-1}$. З
представлень~\eqref{7.7.21} та ~\eqref{7.2.01a} випливає, що
\begin{equation}\label{7.7.25}
\f_1(\l)=q(\l)s(\l)+p(\l)c(\l)(a\l+b)-p(\l)\int_{-\oo}^\oo\frac{c(t)-c(\l)}{t-\l}d\s(t).
\end{equation}

Нехай
$$
A_{j-2d+2}=T_{j-2d+2}(a\l+b):=\begin{bmatrix}
a & b & 0 & \cdots & 0\\
0 & a & b & \cdots & 0\\
0 & 0 & a & \cdots & 0\\
\vdots& &     & \ddots &  \vdots \\
0 & 0 & 0 & \cdots & a
\end{bmatrix}
$$
(відповідно $A=bI$, де $I$ -- це одинична матриця, коли $a=0$,
$b\not=0$; та $A=0$ коли $a=b=0$). Визначимо поліном $s^0$ з
формальним ступенем $\deg s^0=j-d+2$
$$
s^0(\l):=0\cdot\l^{j-d+2}+0\cdot\l^{j-d+1}+s_0^0\l^{j-d}+s^0_1\l^{j-d-1}+...+s^0_{j-d}.
$$
де $\{s_i^0\}_{i=0}^{j-d}$ -- це коефіцієнти з
розкладу~\eqref{7.2.03} для функції $m_{00}\in N^0$. Ми маємо
добавити до полінома $s^0$ нульові доданки так, щоб ступені доданків
$\f_1$ співпадали. Нехай
$$
S_{j-2d+2}^0=T_{j-2d+2}(s^0):=\begin{bmatrix}
0 &0 & s_{0}^0 & s_{1}^0   & \cdots & s_{j-2d-1}^0\\
0 &0        & 0 & s_{0}^0 & \cdots & s_{j-2d-2}^0\\
0 &0        & 0        & 0 & \cdots & s_{j-2d-3}^0\\
\vdots &    &          & \ddots & &  \vdots \\
0 &0        & 0        & 0        & \cdots & 0
\end{bmatrix}.
$$
З Пропозиції~\ref{7.pr:7.1a} випливає, що $\f_1$ є поліномом
формального ступеню $j+n-2d+1$ і
\begin{equation*}\label{7.7.26}
T_{j-2d+2}(\f_1)=Q_{j-2d+2}S_{j-2d+2}+P_{j-2d+2}C_{j-2d+2}A_{j-2d+2}-P_{j-2d+2}C_{j-2d+2}S_{j-2d+2}^0.
\end{equation*}

Факторизація~\eqref{7.3.1} і зображення~\eqref{7.7.21} означають, що
\begin{equation}\label{7.7.27}
-S=P\ol P Q^{-1}(\ol Q)^{-1}(A-S^0),
\end{equation}
де усі матриці мають розмір $(j-2d+2)\times(j-2d+2)$.

З формули~\eqref{7.7.16} для $i=j-2d+2$ і
співвідношення~\eqref{7.7.25} отримаємо,
\begin{equation*}
\begin{aligned}
T(\f_1)&=QP\ol P Q^{-1}(\ol Q)^{-1}(-A+S^0)+P\ol P(\ol Q)^{-1}A-P\ol
P(\ol Q)^{-1}S^0=0.
\end{aligned}
\end{equation*}
Таким чином $\f_1\in\cP_{n-1}$ і формула~\eqref{7.7.13} виконана.

\emph{Нехай $d\leq j<2d-1$.} З умови~\eqref{7.7.22} випливає, що
$s(\l)\equiv0$. Розглянемо зображення вигляду \eqref{7.7.8} для
функції $\l^j m(\l)$, де поліном $c$ визначений формулою
~\eqref{7.7.23}.

Включення $f(\cdot)\in\cH(m_0)$ і $\f_2(\cdot)\in\cP_{\k-1}$
доводяться аналогічно. Розглянемо функцію
$$
\f_1(\l)=p(\l)c(\l)(a\l+b)-p(\l)\int_{-\oo}^\oo\frac{c(t)-c(\l)}{t-\l}d\s(t).
$$
З Пропозиції~\ref{7.pr:7.1a} і умови $j<2d-1$ випливає, що $\f_1$ є
поліномом, та
$$
\deg\f_1=j+k-d+1<d+k=n=\k.
$$
Отже $\f_1\in\cP_{n-1}$, і таким чином відношення~\eqref{7.7.13}
виконано.

\emph{Нехай $j<d$.} З умов~\eqref{7.7.22} випливає, що
$s(\l)\equiv0$. Розглянемо ступінь полінома $p(\l)\l^j$
$$
\deg\bs(){p(\l)\l^j}=k+j<k+d=n=\k.
$$
Як випливає з Теореми~\ref{7.th:3.5} $\l^jm(\l)\in\cH(m)$.

\emph{\textbf{Випадок 3: $\k=\deg p>\deg q$}} є неможливий з
умови~\eqref{7.7.a1}.
\end{proof}

\begin{corollary}\label{7.cor:7.3b}
Нехай функція $m\in N_\k$ задовольняє умові $m(\l)=O(1/\l)$ для
$\l\wh\to\oo$. Визначимо ядро $\mathsf K^m_\mu(\l)$ формулою
\begin{equation}\label{7.7.0''}
\mathsf K^m_{\mu}(\l)=\frac{\l m(\lambda)-\ol\mu
m(\ol\mu)}{\l-\ol\mu}\ \ \ \ (\l,\mu\in\sh_m).
\end{equation}
Тоді
\begin{equation}\label{7.7.1''}
\mathsf K^m_{\mu}(\l)\in\cH(m)
\end{equation}
\end{corollary}

\begin{proof}
Відношення~\eqref{7.7.1''} випливає з тотожності
\begin{equation}\label{7.7.1a''}
\mathsf K^m_{\mu}(\l)=m(\l)+\ol\mu\mathsf
N^m_{\mu}(\l).\end{equation}

Включення $m\in\cH(m)$ доведено в Лемі~\ref{7.lem:7.3a}, а включення
$\mathsf N^m_{\mu}\in\cH(m)$ випливає з означення простору з
відтворювальним ядром.
\end{proof}

